@model POS.ViewModel.OrderModel

@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}

<div class="container">
    <h1></h1>
</div><!-- End Breadcrumbs -->

<section id="contact" class="contact section-bg">
    <div class="container" data-aos="fade-up">
        &nbsp;
        <div class="section-header">
            <h2>Details Order</h2>
        </div>
        <div class="container section-bg" style="overflow: auto;">
            <p></p>
            <table class="table table-bordered" style="width:322px">
                <tr>
                    <th>Id</th>
                    <td>@Model.OrderId</td>
                </tr>
                <tr>
                    <th>Customer Id</th>
                    <td>@Model.CustomerId</td>
                </tr>
                <tr>
                    <th>Employee Id</th>
                    <td>@Model.EmployeeId</td>
                </tr>
                <tr>
                    <th>Order Date</th>
                    <td>@Model.OrderDate</td>
                </tr>
                <tr>
                    <th>Required Date</th>
                    <td>@Model.RequiredDate</td>
                </tr>
                <tr>
                    <th>Ship Via</th>
                    <td>@Model.ShipVia</td>
                </tr>
                <tr>
                    <th>Freight</th>
                    <td>@Model.Freight</td>
                </tr>
                <tr>
                    <th>Ship Name</th>
                    <td>@Model.ShipName</td>
                </tr>
                <tr>
                    <th>Ship Address</th>
                    <td>@Model.ShipAddress</td>
                </tr>
                <tr>
                    <th>Ship City</th>
                    <td>@Model.ShipCity</td>
                </tr>
                <tr>
                    <th>Ship Region</th>
                    <td>@Model.ShipRegion</td>
                </tr>
                <tr>
                    <th>Ship Postal Code</th>
                    <td>@Model.ShipPostalCode</td>
                </tr>
                <tr>
                    <th>Ship Country</th>
                    <td>@Model.ShipCountry</td>
                </tr>
            </table>
        </div>

        <!--End Contact Form -->

    </div>
</section>